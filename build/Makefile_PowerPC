CFLAGS  = -g -O3 -Wall -nostdlib -fno-builtin -pedantic-errors -DCCAPRICE_COMPILING
CCC     = gcc
SRC     = src/x86_64/string.c
OBJ     = $(SRC:.c=.o)
OUT     = ccaprice.a
INC     = -I.

.c.o:
	$(CCC) $(INC) $(CFLAGS) -c $< -o $@
	
$(OUT): $(OBJ)
	ar rcs $(OUT) $(OBJ)
	
clean:
	rm -f $(OBJ) $(OUT)

